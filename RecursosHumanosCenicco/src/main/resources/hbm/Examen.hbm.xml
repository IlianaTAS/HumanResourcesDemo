<?xml version="1.0" encoding="UTF-8" standalone="no"?>

<hibernate-mapping>
    <class name="com.lam.recursoshumanoscenicco.model.Examen" table="EXAMEN">
        <id name="idExamen" type="long">
            <column name="IDEXAMEN" precision="15" scale="0"/>
            <generator class="identity" />
        </id>
        <many-to-one class="com.lam.recursoshumanoscenicco.model.Puesto"  fetch="select" insert="true" name="puesto" not-found="exception"  update="true">
            <column name="IDPUESTO" not-null="true" precision="15" scale="0"/>
        </many-to-one>
        <many-to-one class="com.lam.recursoshumanoscenicco.model.CatalogoParametro"  fetch="select" insert="true" name="catalogoParametro" not-found="exception"  unique="false" update="true">
            <column name="IDCATPARTIPOEXAMEN" not-null="true" precision="15" scale="0"/>
        </many-to-one>
        <property name="descripcion"  type="string">
            <column length="500" name="DESCRIPCION" not-null="true"/>
        </property>
        <property name="duracion" type="string">
            <column length="500" name="DURACION" not-null="true"/>
        </property>
        <set name="preguntas" table="EXAMEN_PREGUNTA"
             inverse="false" lazy="true" fetch="select" cascade="all">
            <key>
                <column name="IDEXAMEN" not-null="true"/>
            </key>
            <many-to-many entity-name="com.lam.recursoshumanoscenicco.model.Pregunta">
                <column name="IDPREGUNTA" not-null = "true"/>
            </many-to-many>  
        </set>
    </class>
</hibernate-mapping>
